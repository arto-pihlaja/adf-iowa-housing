{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ArtoTutorialDf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/FillFactStage')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "FillSalesStage",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "Neighborhood",
											"type": "String"
										},
										"sink": {
											"name": "neighborhood",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "BldgType",
											"type": "String"
										},
										"sink": {
											"name": "bldgtype",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "OverallQual",
											"type": "String"
										},
										"sink": {
											"name": "overallqual",
											"type": "Int32"
										}
									},
									{
										"source": {
											"name": "OverallCond",
											"type": "String"
										},
										"sink": {
											"name": "overallcond",
											"type": "Int32"
										}
									},
									{
										"source": {
											"name": "SaleCondition",
											"type": "String"
										},
										"sink": {
											"name": "salecondition",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "MoSold",
											"type": "String"
										},
										"sink": {
											"name": "mosold",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "YrSold",
											"type": "String"
										},
										"sink": {
											"name": "yrsold",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "SalePrice",
											"type": "String"
										},
										"sink": {
											"name": "saleprice",
											"type": "Decimal"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "HousingTestData",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "SalesStageTable",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SalesStageTable')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SalesStageTable')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ArtoHousingDb",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "neighborhood",
						"type": "varchar"
					},
					{
						"name": "bldgtype",
						"type": "varchar"
					},
					{
						"name": "overallqual",
						"type": "int",
						"precision": 10
					},
					{
						"name": "overallcond",
						"type": "int",
						"precision": 10
					},
					{
						"name": "salecondition",
						"type": "varchar"
					},
					{
						"name": "mosold",
						"type": "varchar"
					},
					{
						"name": "yrsold",
						"type": "varchar"
					},
					{
						"name": "saleprice",
						"type": "money",
						"precision": 19,
						"scale": 4
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "salesstage"
				}
			},
			"dependsOn": []
		}
	]
}